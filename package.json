{
  "name": "squid-evm-template",
  "private": true,
  "scripts": {
    "start": "node -r dotenv/config lib/main.js",
    "start:server": "npx squid-graphql-server",
    "start:server:prod": "npx squid-graphql-server --max-root-fields 10 --dumb-cache in-memory --dumb-cache-ttl 1000 --dumb-cache-max-age 1000",
    "start:queue-admin": "node -r dotenv/config lib/common/workers/index.js",
    "build": "rm -rf lib && tsc",
    "schema:codegen": "npx squid-typeorm-codegen",
    "migration:generate": "npx squid-typeorm-migration generate",
    "migration:apply": "npx squid-typeorm-migration apply",
    "abi:generate": "npx squid-evm-typegen src/abi ./abi/*"
  },
  "dependencies": {
    "@subsquid/evm-abi": "^0.3.0",
    "@subsquid/evm-processor": "^1.19.1",
    "@subsquid/graphql-server": "^4.6.0",
    "@subsquid/typeorm-migration": "^1.3.0",
    "@subsquid/typeorm-store": "^1.5.1",
    "dotenv": "^16.4.5",
    "pg": "^8.12.0",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@subsquid/evm-typegen": "^4.2.0",
    "@subsquid/typeorm-codegen": "^2.0.1",
    "@types/node": "^20.14.8",
    "typescript": "~5.5.2"
  }
}
